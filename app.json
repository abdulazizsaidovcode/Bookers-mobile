{
  "expo": {
    "name": "Bookers",
    "slug": "bookers-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mainLogo.png",
    "scheme": "bookersapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#21212E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alisher09.bookersapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mainLogo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.SCHEDULE_EXACT_ALARM"
      ],
      "package": "com.alisher09.bookersapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos."
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "098d99c4-c6cc-4fd1-85aa-9edd7b778f33",
        "apiBaseUrl": "https://app.bookers.one/"
      }
    }
  }
}