{
  "expo": {
    "name": "Bookers",
    "slug": "notification-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": "bookersapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "cover",
      "backgroundColor": "#21212E",
      "icon": "./assets/images/splash.png"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alisher09.bookersapp"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#000"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.SCHEDULE_EXACT_ALARM"
      ],
      "package": "com.alisher09.bookersapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos."
        }
      ],
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./local/assets/image/splash.png",
          "color": "#ffffff",
          "sounds": [
            "./local/assets/notification-sound.wav"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "9f50422d-d7e6-41fb-b97a-2b512201ab01",
        "apiBaseUrl": "https://app.bookers.one/"
      }
    }
  }
}