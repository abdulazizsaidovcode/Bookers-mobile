{
  "expo": {
    "name": "Bookers",
    "slug": "bookers",
    "owner": "abdulaziziz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "scheme": "bookers",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/loading.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "tabletImage": "./assets/images/loading.png",
      "tabletBackgroundColor": "#ffffff",
      "imageResizeMode": "native",
      "loadingIndicator": "default",
      "loadingIndicatorColor": "#0000ff",
      "hideStatusBar": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "uz.sfera.com"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#000"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.SCHEDULE_EXACT_ALARM"
      ],
      "package": "uz.sfera.com",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA7hQst-XJ1zOl63vgDEfOI05ec3boIPms"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/splash.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location.",
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos."
        }
      ],
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/splash.png",
          "color": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "b3ff7d4a-1888-41a4-866d-1f009521707c",
        "apiBaseUrl": "https://app.bookers.one/"
      }
    }
  }
}